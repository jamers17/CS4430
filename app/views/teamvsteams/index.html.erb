<p id="notice"><%= notice %></p>

<h1>Listing Teamvsteams</h1>

<table>
  <thead>
    <tr>
      <th>Year</th>
      <th>League</th>
      <th>Team</th>
      <th>Opposingteam</th>
      <th>Wins</th>
      <th>Losses</th>
      <th>Ties</th>
      <th>Otl</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teamvsteams.each do |teamvsteam| %>
      <tr>
        <td><%= teamvsteam.year %></td>
        <td><%= teamvsteam.league %></td>
        <td><%= teamvsteam.team %></td>
        <td><%= teamvsteam.opposingTeam %></td>
        <td><%= teamvsteam.wins %></td>
        <td><%= teamvsteam.losses %></td>
        <td><%= teamvsteam.ties %></td>
        <td><%= teamvsteam.otl %></td>
        <td><%= link_to 'Show', teamvsteam %></td>
        <td><%= link_to 'Edit', edit_teamvsteam_path(teamvsteam) %></td>
        <td><%= link_to 'Destroy', teamvsteam, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Teamvsteam', new_teamvsteam_path %>
