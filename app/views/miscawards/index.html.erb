<p id="notice"><%= notice %></p>

<h1>Listing Miscawards</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Award</th>
      <th>Year</th>
      <th>League</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @miscawards.each do |miscaward| %>
      <tr>
        <td><%= miscaward.name %></td>
        <td><%= miscaward.award %></td>
        <td><%= miscaward.year %></td>
        <td><%= miscaward.league %></td>
        <td><%= miscaward.note %></td>
        <td><%= link_to 'Show', miscaward %></td>
        <td><%= link_to 'Edit', edit_miscaward_path(miscaward) %></td>
        <td><%= link_to 'Destroy', miscaward, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Miscaward', new_miscaward_path %>
